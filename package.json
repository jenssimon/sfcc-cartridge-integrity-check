    "npm-run-all": "4.1.5",
    "typescript": "4.6.2"
  },
  "eslintConfig": {
    "extends": [
      "@jenssimon/base",
      "@jenssimon/typescript"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
